<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true"
      throwConfigExceptions="true"
      internalLogLevel="Info"
      internalLogFile="internal-nlog.txt">

  <targets>
    <target xsi:type="File" name="fileTarget" fileName="file.txt" />
    <target xsi:type="Console" name="consoleTarget" />
    <target xsi:type="AssetProcessor.Controls.LogTarget, AssetProcessor" name="memoryTarget" />
  </targets>

  <rules>
    <logger name="*" minlevel="Info" writeTo="fileTarget,consoleTarget,memoryTarget" />
  </rules>
</nlog>
